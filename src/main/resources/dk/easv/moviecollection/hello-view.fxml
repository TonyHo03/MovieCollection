<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="568.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.moviecollection.gui.MainController">
   <children>
   
          <Label fx:id="welcomeText" />
      <Button layoutX="264.0" layoutY="525.0" mnemonicParsing="false" onAction="#btnOnNewMovie" text="Add a movie" />
      <Button layoutX="365.0" layoutY="526.0" mnemonicParsing="false" text="Remove a movie" />
      <Button layoutX="14.0" layoutY="526.0" mnemonicParsing="false" onAction="#btnOnNewCategory" text="Add a category" />
      <Button layoutX="131.0" layoutY="525.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="118.0" text="Remove a category" />
       <Button layoutX="638.0" layoutY="44.0" onAction="#onHelloButtonClick" text="Rate" />
      <TableView fx:id="tblMovies" layoutX="264.0" layoutY="141.0" prefHeight="378.0" prefWidth="414.0">
        <columns>
          <TableColumn fx:id="clmTitle" prefWidth="245.60003662109375" text="Title" />
          <TableColumn fx:id="clmGenre" prefWidth="105.5999755859375" text="Genre" />
            <TableColumn fx:id="clmRating" prefWidth="56.0" text="Score" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ListView fx:id="lstCategories" layoutX="14.0" layoutY="141.0" prefHeight="378.0" prefWidth="233.0" />
      <TextField fx:id="txtFldFilter" layoutX="264.0" layoutY="107.0" prefHeight="26.0" prefWidth="202.0" promptText="&lt;movie name&gt;" />
      <TextField layoutX="482.0" layoutY="44.0" promptText="&lt;Give score&gt;" />
      <Button fx:id="btnSearch" layoutX="471.0" layoutY="108.0" mnemonicParsing="false" onAction="#onSearchBtnClick" text="Search" />
   </children>
</AnchorPane>
